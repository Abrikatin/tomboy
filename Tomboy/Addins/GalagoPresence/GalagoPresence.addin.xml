<Addin id="GalagoPresenceAddin"
	namespace="Tomboy"
	name="Galago Presence"
	author="Tomboy Project"
	description="See online presence of buddies mentioned in a note."
	category="Tools"
	defaultEnabled="false"
	version="0.1">

	<Runtime>
		<Import assembly="GalagoPresence.dll" />
	</Runtime>

	<Dependencies>
		<Addin id="Tomboy" version="0.7" />
	</Dependencies>

	<Extension path="/Tomboy/NoteAddins">
		<NoteAddin type="Tomboy.GalagoPresence.GalagoPresenceNoteAddin" />
	</Extension>
</Addin>
